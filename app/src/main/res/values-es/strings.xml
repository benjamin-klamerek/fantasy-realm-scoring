    <string name="gem_of_order_rules">Bonus: +10 por 3 cartas seguidas, +30 por 4 cartas seguidas, +60 por 5 cartas seguidas, +100 por 6 cartas seguidas, +150 por 7 cartas seguidas</string>
    <string name="warhorse_rules">Bonus: +14 con cualquier Líder o Mago</string>
    <string name="unicorn_rules">Bonus: +30 con la Princesa, +15 con la Emperatriz, Reina, o Encantadora</string>
    <string name="hydra_rules">Bonus: +28 con Pantano</string>
    <string name="dragon_rules">Penalización: -40 a menos que tengas un Mago</string>
    <string name="basilisk_rules"> Penalización: Anula todos los Ejércitos, todos los Líderes y las otras Bestias</string>
    <string name="candle_rules">Bonus: +100 con Libro de los Cambios, Campanario y cualquier Mago</string>
    <string name="fire_elemental_rules">Bonus: +15 por cada otra carta de Fuego</string>
    <string name="forge_rules">Bonus: +9 por cada Arma y Artefacto</string>
    <string name="lightning_rules">Bonus: +30 con Tormenta</string>
    <string name="wildfire_rules">Anula todas las cartas excepto las de Fuego, Mago, Aire, Arma, Artefacto, Montaña, Inundación, Isla, Unicornio y Dragón</string>
