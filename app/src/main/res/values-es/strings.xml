    <string name="water_elemental_rules">Bonus: +15 por cada otra carta de Agua</string>
    <string name="island_rules">Ignora la Penalización de una carta de Agua o de una carta de Fuego</string>
    <string name="swamp_rules">" Penalización: -3 por cada Ejército y Fuego"</string>
